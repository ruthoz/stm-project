#include "button.h"
#include "main.h"




void buttonInit(BUTTON* button , GPIO_TypeDef* GPIOx , uint16_t GPIO_Pin)
{
	button->state = BUTTON_ON;
	button->timetick = 0 ;
	button->GPIOx = GPIOx;
	button->GPIO_Pin = GPIO_Pin;
}


void buttonInterrupt(BUTTON* button)
{
	if(button->state == BUTTON_OFF)
	{
		button->state == BUTTON_ON;


	}
	else
	{
		button->state == BUTTON_OFF;
	}

}

