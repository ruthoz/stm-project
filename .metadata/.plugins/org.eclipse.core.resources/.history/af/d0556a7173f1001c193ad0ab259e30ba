#include "MyMain.h"
#include "led.h"
extern TIM_HandleTypeDef htim6;

LED ledR;
LED ledB;


void HAL_GPIO_EXTI_Callback(uint16_t GPIO_Pin)
{
	ledOnTimerInterrupt(&ledB);
}

void mainloop()
{
	HAL_TIM_Base_Start_IT(&htim6);
	ledInit(&ledB , LD2_GPIO_Port , LD2_Pin );
	ledInit(&ledR , LD3_GPIO_Port , LD3_Pin );

	ledOn(&ledR);
	ledBlink(&ledB, 100);
}
